<toolSet name="External Tools">
  <tool name="Emacs" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="false" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/usr/bin/open" />
      <option name="PARAMETERS" value="-a /Applications/Emacs.app &quot;$FilePath$&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="Terminal" description="Open in external terminal" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="false" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/usr/bin/open" />
      <option name="PARAMETERS" value="-a /Applications/Utilities/Terminal.app &quot;$FileDir$&quot;" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
</toolSet>